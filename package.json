{
  "name": "jupyterlab-interactive-dashboard-editor",
  "version": "0.4.0",
  "description": "Interactively create and customize dashboards in JupyterLab",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "homepage": "https://github.com/jupytercalpoly/jupyterlab-interactive-dashboard-editor",
  "bugs": {
    "url": "https://github.com/jupytercalpoly/jupyterlab-interactive-dashboard-editor/issues"
  },
  "license": "BSD-3-Clause",
  "author": "jupytercalpoly",
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}",
    "style/index.js"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "style": "style/index.css",
  "repository": {
    "type": "git",
    "url": "https://github.com/jupytercalpoly/jupyterlab-interactive-dashboard-editor.git"
  },
  "scripts": {
    "build": "jlpm run build:lib",
    "build:all": "jlpm run build:labextension",
    "build:labextension": "jlpm run clean:all && jlpm run build && jupyter labextension build .",
    "build:lib": "tsc",
    "clean": "jlpm run clean:lib",
    "clean:all": "jlpm run clean:lib && jlpm run clean:labextension",
    "clean:labextension": "rimraf jupyterlab_interactive_dashboard_editor/labextension",
    "clean:lib": "rimraf lib tsconfig.tsbuildinfo",
    "eslint": "eslint . --ext .ts,.tsx --fix",
    "eslint:check": "eslint . --ext .ts,.tsx",
    "prepare": "jlpm run clean && jlpm run build",
    "prettier": "prettier --write \"**/*{.ts,.tsx,.js,.jsx,.css,.json,.md}\"",
    "prettier:check": "prettier --list-different \"**/*{.ts,.tsx,.js,.jsx,.css,.json,.md}\"",
    "watch": "tsc -w"
  },
  "dependencies": {
    "@jupyter-widgets/base": "^4.0.0",
    "@jupyter-widgets/jupyterlab-manager": "^3.0.0",
    "@jupyterlab/application": "^3.0.6",
    "@jupyterlab/apputils": "^3.0.5",
    "@jupyterlab/cells": "^3.0.7",
    "@jupyterlab/coreutils": "^5.0.3",
    "@jupyterlab/docregistry": "^3.0.6",
    "@jupyterlab/launcher": "^3.0.5",
    "@jupyterlab/mainmenu": "^3.0.5",
    "@jupyterlab/notebook": "^3.0.7",
    "@jupyterlab/services": "^6.0.5",
    "@jupyterlab/ui-components": "^3.0.4",
    "@lumino/coreutils": "^1.5.3",
    "@lumino/datastore": "^0.10.3",
    "@lumino/dragdrop": "^1.7.1",
    "@lumino/widgets": "^1.18.0"
  },
  "devDependencies": {
    "@jupyterlab/builder": "^3.0.0-rc.13",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "eslint": "^7.5.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.21.1",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "mkdirp": "^1.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.0",
    "rimraf": "^3.0.2",
    "typescript": "~4.1.3"
  },
  "sideEffects": [
    "style/*.css",
    "style/index.js"
  ],
  "jupyterlab": {
    "extension": "lib",
    "outputDir": "jupyterlab_interactive_dashboard_editor/labextension",
    "sharedPackages": {
      "@jupyter-widgets/base": {
        "bundled": false,
        "singleton": true
      },
      "@jupyter-widgets/jupyterlab-manager": {
        "bundled": false,
        "singleton": true
      }
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --cache --fix"
  },
  "styleModule": "style/index.js"
}
